<h1>Pieces</h1>
<table class="indextable">
  <% @pieces.each do |piece| %>
    <tr>
      <th><%= piece.name %></th>
      <th><%= piece.brand %></th>
      <th><%= piece.size %></th>
      <th><%= piece.colour %></th>
      <th><%= image_tag piece.image%></th>
      <th><%= image_tag piece.image_b%></th>
      <th><a href="#">Book</a>
    </tr>
  <% end %>
</table>
